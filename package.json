{
  "name": "sf-warp",
  "description": "Warp your code and see if your tests notice",
  "version": "0.0.7",
  "dependencies": {
    "@oclif/core": "^1.16.3",
    "@salesforce/core": "^3.25.0",
    "@salesforce/kit": "^1.5.45",
    "@salesforce/sf-plugins-core": "^1.14.1",
    "tslib": "^2",
    "web-tree-sitter-sfapex": "^0.0.9"
  },
  "devDependencies": {
    "@oclif/test": "^2.1.1",
    "@salesforce/cli-plugins-testkit": "^3.0.0",
    "@salesforce/dev-config": "^3.1.0",
    "@salesforce/dev-scripts": "^2",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "1.3.21",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "^35.1.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-sf-plugin": "^1.1.0",
    "husky": "^7.0.4",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "oclif": "^2.7.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "shx": "0.3.4",
    "sinon": "10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/tsQueries"
  ],
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-plugin",
    "sf-plugin",
    "sf"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sf",
    "topicSeparator": " ",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "warp": {
        "description": "Commands to warp your code and test the variations"
      }
    }
  },
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "sf-prepack",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "version": "oclif readme"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Anthony Heber"
}
